{% extends 'base.html.twig' %}

{% block body %}
    Welcome to Demo Theme!

    <br>
    <a href="{{ path('swp_page_about_us') }}">About Us</a>
    <a href="{{ path('swp_page_features') }}">Features</a>
    <a href="{{ path('swp_page_get_involved') }}">Get Involved</a>

    {% container 'container_name' with { 
        'width': 400, 
        'height': 500, 
        'styles': 'border: solid 1px red', 
        'class': 'css_class_name', 
        'data': {'custom-key': value}
    }%}
    <h2>News</h2>
    <ul>
    {% gimmelist article from articles with {'pageName': 'News'} %}
        <li><a href="{{ gimmeUrl(article) }}">{{ article.title }} </a></li>
    {% endgimmelist %}
    </ul>
    {% endcontainer %}
{% endblock %}
